# 예시
from datetime import datetime
dt = datetime(2025, 8, 14, 13, 19, 30)

dt.strftime("%Y-%m-%d %I:%M:%S")
# -> '2025-08-14 13:19:30'

# 예시2
from datetime import datetime
dt = datetime(2025, 8, 14, 1, 19, 30)

fmt = "%Y년 %#m월 %#d일 %p %#I시 %M분 %S초"
print(dt.strftime(fmt))
# -> '2025년 8월 14일 AM 1시 19분 30초'
# %Y : 4자리연도, %y : 2자리연도
